{
  "name": "snap-checker",
  "version": "0.10.2",
  "description": "Synchronized Network Accounting Protocol",
  "main": "src/index.ts",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "hashlocks": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^25.1.3",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "babel-cli": "^6.26.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "typescript": "^3.8.3"
  },
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "build": "babel src -d lib --source-maps --extensions \".ts\"",
    "coverage": "jest --collect-coverage",
    "jest": "jest",
    "lint": "eslint src/ test/ --ext .ts",
    "postversion": "git push && git push --tags && npm publish",
    "prettier": "prettier --write \"{src,test,__mocks__}/**/*.ts\"",
    "preversion": "npm test",
    "test": "npm run build && npm run lint && npm run jest",
    "version": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ledgerloops/snap.git"
  },
  "author": "Michiel de Jong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ledgerloops/snap/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "homepage": "https://ledgerloops.com"
}
